<div class="page-wrapper">
  <h2 class="title"><%= title %></h2>

  <form method="post" action="/admin/navbar/create" class="form-container">
      <!-- <input type="text" name="lang" id="lang" class="form-input" placeholder="Enter language code"> -->
       <datalist>
          <option value="ru"></option>
       </datalist>
      <input type="text" name="label" id="label" class="form-input" placeholder="Enter label">
      <input type="text" name="route" id="route" class="form-input" placeholder="Enter route">

      <button type="submit" class="form-button">Create</button>
  </form>

  <% if (error) { %>
      <p class="error-messages"><%= error %></p>
  <% } %>
</div>
