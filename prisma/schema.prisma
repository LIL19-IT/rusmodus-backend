  createdAt DateTime @default(now())
}

model Contacts_feedback {
  id                   String @id @default(uuid())
  sub_title            String
  sub_title1           String
  route                String
  title                String
  description          String
  checkbox_text        String
  btn_text             String
  placeholder_name     String
  placeholder_phone    String
  placeholder_email    String
  placeholder_question String
  lang                 String
  createdAt            DateTime @default(now())
}

model Contact_address {
  id            String @id @default(uuid())
  lang          String
  title         String
  address       String
  phone_title   String
  phone_number  String
  email_title   String
  email_address String
  createdAt     DateTime @default(now())
}

model Contact_hours {
  id    String @id @default(uuid())
  lang  String
  title String
  days  Days[]
  createdAt DateTime @default(now())
}

model Days {
  id             String        @id @default(uuid())
  day            String
  time           String
  contactHoursId String
  contactHours   Contact_hours @relation(fields: [contactHoursId], references: [id])
  createdAt DateTime @default(now())
}

model Contact_links {
  id    String  @id @default(uuid())
  lang  String
  title String
  links Links[]
  createdAt DateTime @default(now())
}

model Links {
  id             String        @id @default(uuid())
  platform       String
  img_url        String
  url_links      String
  contactLinksId String
  contact_links  Contact_links @relation(fields: [contactLinksId], references: [id])
  createdAt DateTime @default(now())
}

model Questions_and_answers {
  id          String @id @default(uuid())
  sub_title   String
  route       String
  sub_title1  String
  title       String
  description String
  lang        String
  createdAt   DateTime @default(now())
}

