model News_info {
  id         String @id @default(uuid())
  img_url    String
  data       String
  title      String
  link_about String
  categories String
  lang       String
  contents News_content[]
  createdAt DateTime @default(now())

  @@map("news_info")
}

model News_content {
  id           String  @id @default(uuid())
  sub_title    String
  sub_title1   String
  sub_title2   String
  title        String
  data         String
  name         String
  name1        String?
  name2        String?
  name3        String?
  img_url      String
  img1_url     String?
  img2_url     String?
  img3_url     String?
  img4_url     String?
  img5_url     String?
  img6_url     String?
  img7_url     String?
  img8_url     String?
  img11_url    String?
  video_src    String?
  description  String?
  description1 String?
  description2 String?
  description3 String?
  url_text     String?
  url_link     String?
  url_text1    String?
  url_link1    String?
  url_text2    String?
  url_link2    String?
  info_title   String
  news_title   String?
  lang         String
  newsInfo       News_info?       @relation(fields: [newsInfoId], references: [id])
  newsInfoId     String?
  homepageNews   Homepage_news?   @relation(fields: [homepageNewsId], references: [id])
  homepageNewsId String?          
  createdAt      DateTime @default(now())

  @@map("news_content")
}

model News_heading {
  id         String @id @default(uuid())
  sub_title  String
  sub_title1 String
  route      String
  lang       String
  createdAt  DateTime @default(now())
}

model News_categories {
  id   String @id @default(uuid())
  name String
